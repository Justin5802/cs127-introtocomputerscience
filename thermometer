#include <iostream>
using namespace std;

int main()
{
    int temperature;
    cout << "Enter the temperature: ";
    cin >> temperature;
    
    if (temperature <= 32)
        cout << "It's freezing!\n";
    else if(32 < temperature and temperature < 68)
        cout << "It's cold!\n";
    else if(68 < temperature and temperature <  73)
        cout << "It's warm!\n";
    else if(temperature >= 73)
        cout << "It's hot!\n";
    return 0;
}
