#This program prints: your message but 13 letters ahead

mess = input("Please enter a message: ")
print("You entered",mess)


abc = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
newMess = "".join([abc[(abc.find(c)+13)%26] for c in mess])

print("Your message in code is:",newMess)
