#This program prints: Khaki!



print("Khaki!")

import turtle
wn = turtle.Screen()             # Set up the window and its attributes
wn.bgcolor("khaki")


tess = turtle.Turtle()           # create tess and set some attributes
tess.color("darkblue")
tess.pensize(5)

alex = turtle.Turtle()           # create alex
alex.color("violet")
alex.pensize(5)

tess.forward(100)                 # Let tess draw a square
tess.left(90)
tess.forward(100)
tess.left(90)
tess.forward(100)
tess.left(90)
tess.forward(100)
tess.left(90)                     # complete the square

alex.left(90)
alex.forward(100)

alex.right(30)                    #Let alex draw a triangle
alex.forward(100)
alex.right(120)
alex.forward(100)
alex.right(120)
alex.forward(100)               #complete the triangle, complete the house
